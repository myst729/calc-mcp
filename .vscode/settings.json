{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript"
  ],
  "prettier.enable": false,
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true
}
